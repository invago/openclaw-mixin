{
  "id": "openclaw-mixin-channel",
  "name": "Mixin Messenger",
  "description": "将 Openclaw AI 助手接入 Mixin Messenger 平台",
  "version": "1.0.0",
  "kind": "channel",
  "main": "./index.js",
  "configSchema": {
    "type": "object",
    "properties": {
      "MIXIN_APP_ID": {
        "type": "string",
        "description": "Mixin 应用的 App ID",
        "required": true
      },
      "MIXIN_SESSION_ID": {
        "type": "string",
        "description": "Mixin 应用的 Session ID",
        "required": true
      },
      "MIXIN_SESSION_PRIVATE_KEY": {
        "type": "string",
        "description": "会话私钥（Base64 编码）",
        "required": true
      }
    },
    "additionalProperties": false
  }
}
